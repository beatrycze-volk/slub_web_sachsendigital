# -------------------------------
# Language
# -------------------------------
config {
  linkVars = L(1)
  sys_language_uid = 0
  language = de
  htmlTag_langKey = de
  htmlTag_dir = ltr
  htmlTag_setParams = lang="de" dir="ltr" class="no-js"
  locale_all = de_DE.UTF-8
  sys_language_mode = content_fallback
  sys_language_overlay = hideNonTranslated
}

[siteLanguage("languageId") == "1"]
config {
  sys_language_uid = 1
  language = en
  htmlTag_langKey = en
  locale_all = en_GB.UTF8
}
[global]
